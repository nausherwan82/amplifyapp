\t/>\n      <button onClick={createNote}>Create Note</button>\n      <div style={{marginBottom: 30}}>\n        {\n\t\t  notes.map(note => (\n\t\t\t<div key={note.id || note.name}>\n      <h2>{note.name}</h2>\n      <p>{note.description}</p>\n      <button onClick={() => deleteNote(note)}>Delete note</button>\n      {\n        note.image && <img src={note.image} style={{width: 400}} />\n      }\n    </div>\n  ))\n}\n      </div>\n      <AmplifySignOut />\n    </div>\n  );\n}\n\nexport default withAuthenticator(App);","C:\\Development\\react\\amplifyapp\\src\\reportWebVitals.js",[],"C:\\Development\\react\\amplifyapp\\src\\aws-exports.js",[],"C:\\Development\\react\\amplifyapp\\src\\graphql\\queries.js",[],"C:\\Development\\react\\amplifyapp\\src\\graphql\\mutations.js",[],{"ruleId":"31","replacedBy":"32"},{"ruleId":"33","replacedBy":"34"},{"ruleId":"35","severity":1,"message":"36","line":82,"column":23,"nodeType":"37","endLine":82,"endColumn":68},"no-native-reassign",["38"],"no-negated-in-lhs",["39"],"jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","no-global-assign","no-unsafe-negation"]